#include <iostream>
#include <string>
#include <vector>

using namespace std;
using pii = pair<int, int>;
const int INF = 1e9;

vector<vector<int>> graph;

int solution(int n, int s, int a, int b, vector<vector<int>> fares) {
  graph.assign(n + 1, vector<int>(n + 1, INF));
  for (auto& ele : fares) {
    int s = ele[0];
    int e = ele[1];
    int w = ele[2];
    graph[s][e] = w;
    graph[e][s] = w;
  }

  for (int i = 1; i <= n; i++) {
    graph[i][i] = 0;
  }

  for (int k = 1; k <= n; k++) {
    for (int r = 1; r <= n; r++) {
      for (int c = 1; c <= n; c++) {
        graph[r][c] = min(graph[r][c], graph[k][c] + graph[r][k]);
      }
    }
  }

  int answer = INF;
  if (graph[s][a] != INF && graph[s][b] != INF)
    answer = graph[s][a] + graph[s][b];
  for (int i = 1; i <= n; i++) {
    if (i == s) continue;
    if (graph[s][i] == INF || graph[i][a] == INF || graph[i][a] == INF)
      continue;
    answer = min(answer, graph[s][i] + graph[i][a] + graph[i][b]);
  }

  return answer;
}

int main() {
  int n = 6;
  int s = 4;
  int a = 6;
  int b = 2;
  vector<vector<int>> fares = {{4, 1, 10}, {3, 5, 24}, {5, 6, 2},
                               {3, 1, 41}, {5, 1, 24}, {4, 6, 50},
                               {2, 4, 66}, {2, 3, 22}, {1, 6, 25}};

  cout << solution(n, s, a, b, fares) << '\n';

  return 0;
}
